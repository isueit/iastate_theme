		{% endif %}
		{% if content.field_lblock_link04.0['#title'] %}
			<a aria-labelledby="{{ content.field_lblock_link04.0['#title'] }}" id="{{ content.field_lblock_link04.0['#title'] }} Link" class="links_block_field" href="{{ content.field_lblock_link04.0['#url'] }}">
				{% if content.field_lblock_icon04 %}
						{{ content.field_lblock_icon04 }}
				{% endif %}
				<p>{{ content.field_lblock_link04.0['#title'] }}</p>
			</a>
		{% endif %}
		{% if content.field_lblock_link05.0['#title'] %}
			<a aria-labelledby="{{ content.field_lblock_link05.0['#title'] }}" id="{{ content.field_lblock_link05.0['#title'] }} Link" class="links_block_field" href="{{ content.field_lblock_link05.0['#url'] }}">
				{% if content.field_lblock_icon05 %}
