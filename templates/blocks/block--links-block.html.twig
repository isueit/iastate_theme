		{% endif %}
		{% if content.field_lblock_link06.0['#title'] %}
			<a aria-labelledby="{{ content.field_lblock_link06.0['#title'] }}" id="{{ content.field_lblock_link06.0['#title'] }} Link" class="links_block_field" href="{{ content.field_lblock_link06.0['#url'] }}">
				{% if content.field_lblock_icon06 %}
						{{ content.field_lblock_icon06 }}
				{% endif %}
				<p>{{ content.field_lblock_link06.0['#title'] }}</p>
			</a>
		{% endif %}
		{{ title_suffix }}
	</div>

{% endblock %}
